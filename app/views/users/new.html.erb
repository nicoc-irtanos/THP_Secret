<html>
  <body>
    <h1>Inscris-toi</h1>
      <%= form_tag('/users', method: "post") do %>
      <%= label_tag "Email" %>
      <br>
      <%= email_field_tag :email %>
      <br>
      <%= label_tag "Mot de passe" %>
      <br>
      <%= password_field_tag :password %>
      <br>
      <%= label_tag "Confirmer mot de passe" %>
      <br>
      <%= password_field_tag :password_confirmation %>
      <br>
      <%= submit_tag "Je m'inscris" %>

      <% end %>
  </body>
</html>
